{
	"mode": "pullwatch",
    "url": "https://github.com/kelvv/test.git",
    "localdir": "/Users/jarvin/Desktop/test",
    "interval":3000,
    "branch":"master",
    "rules": [
        {
            "type": "tag",
            "value": "[vV]\\d+",
